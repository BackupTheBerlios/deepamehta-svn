<project name="DeepaMehta-db-dropdb-hsqldb">
	<import file="dropdb-ask.xml"/>
	<target name="-dropdb-do" depends="-askdropdb" unless="do.abort">
		<delete failonerror="false">
			<fileset dir="${db.hsqldb.base-path}">
				<include name="${db.name}.*" />
			</fileset>
		</delete>
	</target>
</project>
